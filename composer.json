{
    "name": "abelhalo/api-proxy",
    "description": "A simple api proxy for Laravel",
    "keywords": [
        "laravel"
    ],
    "homepage": "https://github.com/zxz054321/api-proxy",
    "authors": [
        {
            "name": "Abel",
            "email": "zxz054321@foxmail.com",
            "homepage": "https://github.com/zxz054321"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "AbelHalo\\ApiProxy\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "ApiProxy": "AbelHalo\\ApiProxy\\Facade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
